{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(jira sprint:*)",
      "Bash(jira sprint list:*)",
      "Bash(jira me:*)"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/.claude/configs/hooks/notifications/confirm.mp3"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/.claude/configs/hooks/notifications/success.mp3"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "claude@cc-plugins": true,
    "git@cc-plugins": true,
    "jira-tools@acm-workflows-plugins": false
  },
  "alwaysThinkingEnabled": false,
  "feedbackSurveyState": {
    "lastShownTime": 1754274455319
  }
}
